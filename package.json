{
  "name": "react-aws-s3-uploader",
  "version": "1.0.0",
  "description": "Simple React AWS S3 Uploader",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gndx/react-aws-s3-uploader.git"
  },
  "keywords": [
    "react",
    "AWS",
    "S3",
    "file",
    "file-upload",
    "s3-upload",
    "react-s3"
  ],
  "author": {
    "name": "Oscar Barajas",
    "email": "oscar@arepa.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gndx/react-aws-s3-uploader/issues"
  },
  "homepage": "https://github.com/gndx/react-aws-s3-uploader#readme",
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  }
